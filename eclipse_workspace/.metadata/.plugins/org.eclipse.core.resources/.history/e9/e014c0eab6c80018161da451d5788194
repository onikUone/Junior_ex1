package eclipse_ex1;
import java.util.Arrays;

public class Neuron {
	//member field
	double[] weight;
	double threshoud;
	double net;
	double eta;
	double alpha;
	double[] delta_W;
	double delta_T;

	Neuron(int preNumber){
		weight = new double[preNumber];
		for(int i=0; i<preNumber; i++){
			weight[i] = 0.5;
		}
		threshoud = 0.5;
		eta = 0.5;
		alpha = 0.9;
		delta_W = new double[weight.length];
		Arrays.fill(delta_W, 0.0);
		delta_T = 0.0;
	}
}
